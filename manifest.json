{
  "manifest_version": 2,
  "name": "Tab Deduplicator",
  "version": "1.0.1",
  "description": "Automatically detect duplicate tabs, close old tabs and keep newly opened tabs",
  "author": "BH Lin",
  "homepage_url": "https://github.com/binghuan/Tab-Deduplicator",
  "browser_specific_settings": {
    "gecko": {
      "id": "tab-deduplicator@binghuan.github.io",
      "strict_min_version": "142.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "icons": {
    "48": "icons/icon-48.svg",
    "96": "icons/icon-96.svg"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-48.svg",
      "38": "icons/icon-96.svg"
    },
    "default_title": "Tab Deduplicator",
    "default_popup": "popup/popup.html"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  }
}
